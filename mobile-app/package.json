  "private": true,
  "expo": {
    "name": "StarPrint CRM",
    "slug": "starprint-crm",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3b82f6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.starprint.crm",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Este app precisa acessar a câmera para escanear QR codes e capturar fotos de documentos.",
        "NSPhotoLibraryUsageDescription": "Este app precisa acessar a galeria para selecionar imagens de documentos.",
        "NSLocationWhenInUseUsageDescription": "Este app precisa acessar a localização para funcionalidades de mapeamento.",
        "NSMicrophoneUsageDescription": "Este app precisa acessar o microfone para gravação de áudio.",
        "NSFaceIDUsageDescription": "Este app usa Face ID para autenticação segura.",
        "UIBackgroundModes": [
          "remote-notification",
          "background-fetch",
          "background-processing"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3b82f6"
      },
      "package": "com.starprint.crm",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.VIBRATE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.WAKE_LOCK",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-notifications",
      "expo-camera",
      "expo-image-picker",
      "expo-location",
      "expo-sensors",
      "expo-haptics",
      "expo-av",
      "expo-speech",
      "expo-barcode-scanner",
      "expo-file-system",
      "expo-sharing",
      "expo-print",
      "expo-updates"
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    },
    "owner": "starprint",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/your-project-id"
    }
  },
  "eas": {
    "projectId": "your-project-id",
    "cli": {
      "version": ">= 5.9.1"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal"
      },
      "preview": {
        "distribution": "internal"
      },
      "production": {
        "autoIncrement": true
      }
    },
    "submit": {
      "production": {}
    }
  }
}
